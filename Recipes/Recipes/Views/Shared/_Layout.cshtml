    <link rel="stylesheet" href="~/Content/ostrich-sans-fontfacekit/stylesheet.css" type="text/css">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
